<%= provide(:title) %>

<h1>イベント一覧</h1>
<div class="row">
  <div class="col-md-3">
    <%= render 'search' %>
  </div>
  <div class="col-md-9">
    <div class="row">
      <% @events.each do |event| %>
        <%= render 'feed', event: event %>
      <% end %>
    </div>
    <%= paginate @events %>
  </div>
</div>