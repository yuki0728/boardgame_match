<%= f.label :name %>
<%= f.text_field :name, class: 'form-control' %>

<%= f.label :text %>
<%= f.text_field :text, class: 'form-control' %>

<%= f.label :start_time %>
<%= f.datetime_select :start_time, use_month_numbers: true, class: 'time-box' %>

<%= f.label :ending_time %>
<%= f.datetime_select :ending_time, use_month_numbers: true, class: 'time-box' %>

<%= f.label :participant_limit %>
<%= f.number_field :participant_limit %>

<%= f.label :tag_list %>
<%= f.text_field :tag_list, value: @event.tag_list.join(','), class: "form-control" %>

<%= f.submit %>